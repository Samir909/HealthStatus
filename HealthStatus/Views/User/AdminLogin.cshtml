@model List<HealthStatus.DomainModels.StatusShow>
@{
    ViewBag.Title = "AdminLogin";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>AdminLogin</h2>

<div class="row">
    <div class="col-sm-12">
        <table class="table">

            <th>USER</th>
            <th>STATUS</th>
            <th>DATE</th>
            <th>COMMENTS</th>
            @foreach (var item in Model)
            {
                Html.RenderPartial("UserListView", item);
            }



        </table>

    </div>
</div>